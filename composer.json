{
  "name": "route4me/route4me-php",
  "description": "Access Route4Me's logistics-as-a-service API using our PHP SDK",
  "minimum-stability": "stable",
  "version": "1.1.0",

  "authors": [
    {
    "name": "Guchashvili Valeri",
    "email": "valesopia@gmail.com"
    },
	{
    "name": "Guchashvili Oleg",
    "email": "olegi_g@hotmail.com"
    }
  ],
  "repositories": [
	{
		"type": "package",
		"package": {
		  "name": "r4m",
		  "version": "0.1.0",
		  "type": "package",
		  "source": {
			"url": "https://github.com/route4me/route4me-php-sdk",
			"type": "git",
			"reference": "master"
		  }
		}
	},
	{
		"type": "package",
		"package": {
		  "name": "companies/onfleet",
		  "version": "0.1.0",
		  "type": "package",
		  "source": {
			"url": "https://github.com/anorgan/onfleet-client",
			"type": "git",
			"reference": "master"
		  }
		}
	},
	{
		"type": "package",
		"package": {
		  "name": "companies/onfleet/delivery-manager",
		  "version": "0.1.0",
		  "type": "package",
		  "source": {
			"url": "https://github.com/lhcboinc/delivery-manager",
			"type": "git",
			"reference": "master"
		  }
		}
	},
	{
		"type": "package",
		"package": {
		  "name": "companies/onfleet/php-wrapper",
		  "version": "0.1.0",
		  "type": "package",
		  "source": {
			"url": "https://github.com/billybateman/Onfleet-PHP",
			"type": "git",
			"reference": "master"
		  }
		}
	}
  ],

  "require": {
    "php": ">=5.3.0",
	"r4m": "*",
	"companies/onfleet": "*",
	"companies/onfleet/delivery-manager": "*",
	"companies/onfleet/php-wrapper": "*"
  },

  "require-dev": {
    "phpunit/phpunit": "4.0"
  },

  "autoload": {
    "psr-0": {
      "Mag": "src/"
    }
  }
}
